<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tian.myweb.sys.dao.UserDAO">

	<insert id="insertUser" parameterType="com.tian.myweb.sys.model.UserModel" keyProperty="id">
		insert into user(password, username)
		values
		( #{password}, #{username})
	</insert>

</mapper>